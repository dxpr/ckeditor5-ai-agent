msgid "Simplify Language"
msgstr "Vereenvoudig taal"

msgctxt "Generate from selection"
msgid "Generate from selection"
msgstr "Genereren uit selectie"

msgctxt "Summarize"
msgid "Summarize"
msgstr "Samenvatten"

